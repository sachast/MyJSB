{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Personne</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ entity.prenom }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ entity.statut }}</td>
            </tr>
            <tr>
                <th>Creele</th>
                <td>{{ entity.creeLe|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Validele</th>
                <td>{{ entity.valideLe|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Premisdeconduire</th>
                <td>{{ entity.premisDeConduire }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('inscription') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('inscription_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
